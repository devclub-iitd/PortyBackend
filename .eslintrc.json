{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": ["plugin:prettier/recommended", "airbnb-base", "prettier"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "plugins": ["prettier"],
    "rules": {
        "no-console": "off", // TODO: Find better Console object
        "camelcase": "off"
    }
}